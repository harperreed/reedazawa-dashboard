<script type='text/javascript'>
$(function() {

  Dashing.numColumns = 3
  Dashing.cycleDashboards({timeInSeconds: 15, stagger: true});
});
</script>

<% content_for :title do %>Reedazawa Dashboard<% end %>

<div class="gridster">
<ul>
 <li data-row="1" data-col="1" data-sizex="3" data-sizey="2">
  <div data-id="picture" data-view="BigImage" data-image="http://i.imgur.com/kVdZcgW.jpg" style="background-color:transparent;"data-max="true">  </div>
  </li>
  </ul>
</div>

<div class="gridster">
  <ul>
<li data-row="1" data-col="1" data-sizex="3" data-sizey="2">
      <div data-id="forecast" data-view="Forecast" data-widget_title="<img src='http://forecast.io/favicon.ico' />Weather Forecast"> ></div>
    </li>
  </ul>
  </div>

<div class="gridster">
  <ul>
     <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="buzzwords" data-view="List" data-unordered="true" data-title="Buzzwords" data-moreinfo="# of times said around the office"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="convergence" data-view="Graph" data-title="Convergence" style="background-color:#ff9618"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="valuation" data-view="Number" data-title="Current Valuation" data-moreinfo="In billions" data-prefix="$"></div>
    </li>
  </ul>
</div>